<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/Scenes/UIToolkit/StyleSheets/MainStyles.uss?fileID=7433441132597879392&amp;guid=fbd390f23a7ea9f4ba95eaeb258fced3&amp;type=3#MainStyles" />
    <ui:VisualElement name="songEntryUiRoot" class="roundCorners" style="width: 100%; height: 100%; position: absolute; background-color: rgb(6, 170, 233); max-height: 200px; max-width: 150px;">
        <ui:VisualElement name="songImageOuter" picking-mode="Ignore" style="background-image: url(&apos;project://database/Assets/Common/Graphics/Icons/NoCover.jpg?fileID=2800000&amp;guid=1d183c612f1866943a54609ced7fece7&amp;type=3#NoCover&apos;); -unity-background-scale-mode: scale-and-crop; flex-grow: 10;">
            <ui:VisualElement name="songImageInner" picking-mode="Ignore" style="background-image: url(&apos;project://database/Assets/Common/Graphics/Icons/NoCover.jpg?fileID=2800000&amp;guid=1d183c612f1866943a54609ced7fece7&amp;type=3#NoCover&apos;); -unity-background-scale-mode: scale-to-fit; flex-grow: 1; display: none;" />
            <ui:VisualElement name="songPreviewBackgroundImage" picking-mode="Ignore" style="position: absolute; width: 100%; height: 100%;" />
        </ui:VisualElement>
        <ui:VisualElement name="songInfo" picking-mode="Ignore" class="smallFont" style="height: 25%; padding-left: 5px; padding-right: 5px; padding-top: 5px; padding-bottom: 0; flex-shrink: 0; min-height: 50px; flex-grow: 1;">
            <ui:Label text="Some very very long artist name" display-tooltip-when-elided="true" name="songArtist" picking-mode="Ignore" class="ellipsis" style="-unity-font-style: bold; margin-bottom: 5px; max-height: 33px;" />
            <ui:Label text="Some very very long song title" display-tooltip-when-elided="true" name="songTitle" picking-mode="Ignore" class="ellipsis" style="width: 90%; flex-grow: 1; white-space: normal; max-height: 66%;" />
            <ui:VisualElement name="titleRow" picking-mode="Ignore" style="flex-direction: row; justify-content: space-between; align-items: center;" />
        </ui:VisualElement>
        <ui:VisualElement name="iconBar" picking-mode="Ignore" style="position: absolute; flex-shrink: 0; right: 5px; top: 5px; align-items: center;">
            <ui:VisualElement name="songEntryFavoriteIcon" picking-mode="Ignore" style="background-image: url(&apos;project://database/Assets/Common/Graphics/Icons/MaterialDesignIcons/favorite_white_24dp.svg?fileID=7388822144124034973&amp;guid=5fd6f255d5637614eae7dfe140b680d7&amp;type=3#favorite_white_24dp&apos;); width: 12px; height: 12px; flex-shrink: 0; display: flex; -unity-background-scale-mode: scale-to-fit;" />
            <ui:VisualElement name="songEntryDuetIcon" picking-mode="Ignore" style="background-image: url(&apos;project://database/Assets/Common/Graphics/Icons/DuetIcon.png?fileID=21300000&amp;guid=ecea42b89d4de5f42a25fd1a598b26a1&amp;type=3#DuetIcon&apos;); width: 12px; height: 12px; flex-shrink: 0; display: flex; -unity-background-scale-mode: scale-to-fit;" />
        </ui:VisualElement>
        <ui:VisualElement name="songMenuOverlay" class="focusableNavigatorRoot" style="position: absolute; width: 100%; height: 100%; background-color: rgba(32, 32, 32, 0.88); display: none;">
            <ui:ScrollView scroll-deceleration-rate="0,135" elasticity="0,1" name="songMenuOverlayScrollView" style="width: 100%; height: 100%; align-items: center;">
                <ui:Button text="Sing" display-tooltip-when-elided="true" name="singThisSongButton" style="width: 100%;" />
                <ui:VisualElement name="modifyPlaylistButtonContainer" style="width: 100%;" />
                <ui:Button text="Editor" display-tooltip-when-elided="true" name="openSongEditorButton" style="width: 100%;" />
                <ui:Button text="Reload" display-tooltip-when-elided="true" name="reloadSongButton" style="width: 100%;" />
                <ui:Button text="Open Folder" display-tooltip-when-elided="true" name="openSongFolderButton" style="width: 100%;" />
                <ui:Button text="Back" display-tooltip-when-elided="true" name="closeSongOverlayButton" style="width: 100%;" />
            </ui:ScrollView>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
